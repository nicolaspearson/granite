{
  "name": "svc-consents",
  "private": true,
  "version": "0.0.0",
  "description": "The consent management service",
  "keywords": [
    "backend"
  ],
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/nicolaspearson/granite.git"
  },
  "author": "Nicolas Pearson <nic.s.pearson@gmail.com>",
  "license": "MIT",
  "directories": {
    "src": "src",
    "test": "test"
  },
  "files": [
    "src/**/*"
  ],
  "scripts": {
    "build": "rimraf dist && yarn exec:tsc --build $(pwd)/tsconfig.build.json",
    "docs": "yarn exec:typedoc --tsconfig $(pwd)/tsconfig.build.json --out $(pwd)/typedoc $(pwd)/src",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "yarn exec:eslint --parser-options \"{ tsconfigRootDir: $(pwd), project: '$(pwd)/tsconfig.json' }\" --cache-file $(pwd)/.eslintcache $(pwd)/src $(pwd)/test",
    "start:debug": "yarn start:dev --debug",
    "start:dev": "nest start -p tsconfig.build.json --watch",
    "start:docker": "yarn exec:docker-compose up -d --build svc-consents && yarn exec:docker-compose logs -f svc-consents",
    "start:prod": "node dist/main",
    "start": "node dist/main.js",
    "test": "yarn test:unit && yarn test:integration",
    "test:integration": "yarn exec:jest --config $(pwd)/test/integration/jest.config.js",
    "test:unit": "yarn exec:jest --config $(pwd)/test/unit/jest.config.js"
  },
  "dependencies": {
    "@nestjs/common": "^8.2.3",
    "@nestjs/config": "^1.1.3",
    "@nestjs/core": "^8.2.3",
    "@nestjs/platform-express": "^8.2.3",
    "@nestjs/swagger": "^5.1.5",
    "@nestjs/typeorm": "^8.0.2",
    "common-tags": "^1.8.2",
    "helmet": "^4.6.0",
    "joi": "^17.4.2",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.4.0",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.2.41"
  },
  "devDependencies": {
    "#": "link:./test",
    "$": "link:./src",
    "@nestjs/cli": "^8.1.5",
    "@nestjs/schematics": "^8.0.5",
    "@nestjs/testing": "^8.2.3",
    "@types/common-tags": "^1.8.1",
    "@types/express": "^4.17.13",
    "@types/helmet": "^4.0.0",
    "@types/jest": "27.0.3",
    "@types/node": "^16.11.10",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "granite": "workspace:*",
    "jest": "^27.3.1",
    "prettier": "^2.4.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "4.3.5",
    "webpack": "^5.64.3"
  }
}
